# Node Development Mode (Development, Production)
NODE_ENV=development
# NODE_ENV=production

# MongoDB Database
MONGO_DATABASE=vmv-test-database

# MongoDB Host Location (Local Machine With Docker, Docker Container, Local Machine Only)
MONGO_HOST_LOCATION=host.docker.internal
# MONGO_HOST_LOCATION=mongo
# MONGO_HOST_LOCATION=localhost

# MongoDB Listening Port
MONGO_PORT=27017

# Mongo Express Database Reference
ME_CONFIG_MONGODB_SERVER=vmv-mongodb-dev

# Mongo Express Listening Port
MONGO_EXPRESS_PORT=8081

# Server Listening Port
SERVER_PORT=8000
